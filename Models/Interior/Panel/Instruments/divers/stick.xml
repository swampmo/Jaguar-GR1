	<!-- Lighting -->
	<animation> 
		<object-name>Jag-FS</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.275</red>
			<green>0.12</green>
			<blue>0.05</blue>
		</emission>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>35</max-m>
	</animation>

