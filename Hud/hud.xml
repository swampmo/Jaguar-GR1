 <!-- heading -->

    <label>
	<x>5</x>
    <y>-60</y> 
		<width>60</width>
		<height>10</height>
		<box>false</box>
		<format>ROLL</format>
		<halign>left</halign>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>wing-leveler</value>
			</equals>
		</condition>
	</label>
    

	<label>
	<x>5</x>
    <y>-60</y>
		<width>60</width>
		<height>10</height>
		<box>false</box>
		<format>HDG %03d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/heading-bug-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>dg-heading-hold</value>
			</equals>
		</condition>
	</label>

	<label>
	<x>5</x>
    <y>-60</y> 
		<width>60</width>
		<height>10</height>
		<box>false</box>
		<format>HDG %03d</format>
		<halign>left</halign>
		<input>
			<property>/autopilot/settings/true-heading-deg</property>
		</input>
		<condition>
			<equals>
				<property>/autopilot/locks/heading</property>
				<value>true-heading-hold</value>
			</equals>
		</condition>
	</label>


